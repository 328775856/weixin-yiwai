page {
  height: 100%;
}

.iconRightWrp {
  position: fixed;
  width: 458rpx;
  height: 415rpx;
  right: 0;
  top: 0;
}

#productWrp {
  /* background-image: url(https://img.kanhua.yiwaiart.com/miniApp/activity/EY_bg_pink@1x.png); */
  background-repeat: no-repeat;
  background-position: top center;
  background-size: cover;
  /* background-color: linear-gradient(to bottom, #ffd9e3, #fff5f8); *//* padding-top: 30rpx;
  padding-bottom: 80rpx; *//* box-sizing: border-box; */
  background-color: #ffdee7;
  height: 100%;
}

.svWrp {
  height: calc(100% - 130rpx);
  width: 100%;
  position: relative;
  top: 130rpx;
}

.searchOutterWrp {
  position: fixed;
  width: 100%;
  top: 30rpx;
  left: 0;
  z-index: 9999;
}

.searchWrp {
  height: 60rpx;
  /* background-color: #fedbe6; *//* background-color: rgb(253, 214, 227); */
  background-color: #fec3d5;
  /* background-color: red; *//* background-color: red;  */
  border-radius: 30rpx;
  overflow: hidden;
}

.searchWrp input {
  color: #c65977;
  font-size: 24rpx;
  /* background-color: #fedbe6; */
  width: 100%;
  padding-right: 30rpx;
  box-sizing: border-box;
}

.searchWrp .searchIcon {
  width: 26rpx;
  height: 26rpx;
  margin: 0 30rpx;
}

.productListWrp {
  margin: 0 30rpx;
  /* padding-top: 90rpx; */
  padding-bottom: 80rpx;
}

.mlr30 {
  margin-left: 30rpx;
  margin-right: 30rpx;
}

/* productItem模板 begin */

.productItemWrp {
  margin-top: 40rpx;
}

.productItemWrp:first-child {
  margin-top: 0;
}

.productItemWrp .imgWrp {
  position: relative;
}

.productItemWrp .imgProduct {
  width: 100%;
  height: 380rpx;
  vertical-align: middle;
  background-image: url(https://img.kanhua.yiwaiart.com/picture/default_bg_2.png);
  background-repeat: no-repeat;
  background-position: center;
  background-size: cover;
}

.productItemWrp .numWrp {
  width: 104rpx;
  height: 52rpx;
  line-height: 52rpx;
  text-align: center;
  position: absolute;
  top: 0;
  left: 0;
  background: #fff;
  border-radius: 0 0 6rpx 0;
  font-size: 32rpx;
}

.productItemWrp .typeWrp {
  font-size: 22rpx;
  position: absolute;
  text-align: center;
  line-height: 38rpx;
  /* width: 88rpx; */
  height: 38rpx;
  background: rgba(0, 0, 0, 0.6);
  border-radius: 19rpx;
  right: 20rpx;
  bottom: 20rpx;
  color: #fff;
}

.productItemWrp .iconPeach {
  width: 33rpx;
  height: 34rpx;
  margin-right: 10rpx;
}

.productItemWrp .iconLike {
  width: 120rpx;
  height: 46rpx;
  margin-left: 40rpx;
}

.productItemWrp .allWrp {
  color: #c87561;
  font-size: 22rpx;
}

.productItemWrp .t1 {
  color: #2f2f2f;
  font-size: 32rpx;
  line-height: 1;
}

.productItemWrp .t2 {
  color: #2f2f2f;
  font-size: 24rpx;
  line-height: 1;
  margin-top: 10rpx;
}

.productItemWrp .btmWrp {
  background-color: #fff;
  padding-top: 30rpx;
  padding-bottom: 20rpx;
  padding-left: 30rpx;
  padding-right: 30rpx;
}

/* productItem模板 end */

.noDataWrp {
  font-size: 24rpx;
  text-align: center;
}
